<?xml version="1.0"?>
<openerp>
    <data>

        <!-- Add external_id to res.partner -->
        <record id="view_partners_form_salesinq" model="ir.ui.view">
                <field name="name">view.res.partner.form.salesinq</field>
                <field name="model">res.partner</field>
                <field name="inherit_id" ref="base.view_partner_form"/>
                <field eval="18" name="priority"/>
                <field name="arch" type="xml">
                    <page string="History" position="after">
                        <page string="SalesInq" attrs="{'invisible': [('is_salesinq_able','=',False)]}">
                            <field name="is_salesinq_able" invisible="1"/>
                            <field name="salesinq_data" nolabel="1" class="salesinq"/>
                        </page>
                     </page>

                </field>
        </record>

    </data>
</openerp>
