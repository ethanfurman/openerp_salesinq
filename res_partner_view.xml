<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <!--
         |  Add external_id to res.partner
        -->
        <record id="view_partners_form_salesinq" model="ir.ui.view">
            <field name="name">view.res.partner.form.salesinq</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="priority" eval="18"/>
            <field name="arch" type="xml">
                <page position="after" string="History">
                    <page attrs="{'invisible': [('is_salesinq_able','=',False)]}" string="SalesInq">
                        <field name="is_salesinq_able" invisible="1"/>
                        <field name="salesinq_data" class="salesinq" nolabel="1"/>
                    </page>
                </page>
            </field>
        </record>

    </data>
</openerp>